---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Jennifer Smith's blog</title>
 <link href="http://jennifersmith.github.com/atom.xml" rel="self"/>
 <link href="http://jennifersmith.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://jennifersmith.github.com</id>
 <author>
   <name>Jennifer Smith</name>
 </author>
 
 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://jennifersmith.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.paperplanes.de{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
